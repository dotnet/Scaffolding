<Project>
  <PropertyGroup>
    <InternalAspNetCoreSdkVersion>2.0.0-*</InternalAspNetCoreSdkVersion>
    <NETStandardImplicitPackageVersion>$(BundledNETStandardPackageVersion)</NETStandardImplicitPackageVersion>
  </PropertyGroup>
  <!-- AspNetCore Versions -->
  <PropertyGroup>
    <AspNetCoreVersion>2.0.0-*</AspNetCoreVersion>
    <DotNetInternalAbstractionsVersion>1.0.0</DotNetInternalAbstractionsVersion>
  </PropertyGroup>
  <!-- Partner Versions-->
  <PropertyGroup>
    <CodeAnalysisVersion>2.0.0</CodeAnalysisVersion>
    <JsonNetVersion>10.0.1</JsonNetVersion>
    <!-- This needs to match the CLI's msbuild version. 
      https://github.com/dotnet/cli/blob/master/build/DependencyVersions.props
      -->
    <MicrosoftBuildVersion>15.2.0-preview-000093-02</MicrosoftBuildVersion>
    <NuGetFrameworksVersion>4.0.0-*</NuGetFrameworksVersion>
    <CoreFxVersion>4.3.0</CoreFxVersion>
  </PropertyGroup>
  <!-- Dependencies for Test Project -->
  <PropertyGroup>
    <CliUtilsVersion>1.0.0-preview4-004233</CliUtilsVersion>
    <TestSdkVersion>15.0.0</TestSdkVersion>
    <MoqVersion>4.6.36-*</MoqVersion>
    <XunitVersion>2.2.0</XunitVersion>
    <DotNetProjectModelVersion>1.0.0-rc3-003121</DotNetProjectModelVersion>
  </PropertyGroup>
  <!-- MSBuild used by CLI currently uses the 9.0.1 version. -->
  <PropertyGroup>
    <JsonNetLegacyVersion>9.0.1</JsonNetLegacyVersion>
  </PropertyGroup>
</Project>
