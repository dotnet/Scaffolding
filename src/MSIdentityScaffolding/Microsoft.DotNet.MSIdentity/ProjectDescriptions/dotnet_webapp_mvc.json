{
  "Identifier": "dotnet-webapp-mvc",
  "ProjectRelativeFolder": "",
  "BasedOnProjectDescription": "dotnet-web",
  "MatchesForProjectType": [
    {
      "FileRelativePath": "Program.cs",
      "MatchAny": [ "AddControllersWithViews" ]
    },
    {
      "FolderRelativePath": "Models",
      "Sets": "IsWebApp"
    },
    {
      "FolderRelativePath": "Views",
      "Sets": "IsWebApp"
    }
  ],
  "BasePackages": [
    "Microsoft.AspNetCore.Authentication",
    "Microsoft.AspNetCore.Authentication.OpenIdConnect",
    "Microsoft.AspNetCore.Authorization"
  ],
  "CommonPackages": [
    "Microsoft.Identity.Web",
    "Microsoft.Identity.Web.UI"
  ],
  "MicrosoftGraphPackages": [
    "Microsoft.AspNetCore.Authentication.OpenIdConnect",
    "Microsoft.AspNetCore.Authentication.JwtBearer",
    "Microsoft.Identity.Web.MicrosoftGraph"
  ],
  "DownstreamApiPackages": [
    "Microsoft.AspNetCore.Authentication.OpenIdConnect",
    "Microsoft.AspNetCore.Authentication.JwtBearer"
  ]
}
